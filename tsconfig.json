{
"compileOnSave": true,
"compilerOptions": {
  "module": "commonjs",
  "lib": [
    "es2020",
    "es2021",
    "dom"
  ],
  "target": "es2020",
  "removeComments": false,
  "preserveConstEnums": true,
  "sourceMap": true,
  "inlineSources": true,
  "declaration": true,
  "resolveJsonModule": true,
  "downlevelIteration": true,
  "skipLibCheck": true,
  "strict": true,
  "strictFunctionTypes": false,
  "strictPropertyInitialization": false,
  "outDir": "jsFiles"
},
"exclude": [
  "**/node_modules",
  "**/test/*",
  "./jsFiles/**/*"
]
}
